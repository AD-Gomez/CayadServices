---
import ShowDataQuote from "../../components/quote2/ShowData";
import ShowDestination from "../../components/quote2/ShowDestination";
import ShowNumberLead from "../../components/quote2/ShowNumberLead";
---
<!-- End Google Tag Manager (noscript) -->
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta
      name="description"
      content="Cayad Auto Transport provides the most cost effective car shipping services across the US. Get your quote today!"
    />
    <meta name="viewport" content="width=device-width" />
    <link
      rel="icon"
      type="image/webp"
      href="/img/title-icon.webp"
      sizes="120x120"
    />
    <script is:inline>
      (function (w, d, s, l, i) {
        w[l] = w[l] || [];
        w[l].push({ "gtm.start": new Date().getTime(), event: "gtm.js" });
        var f = d.getElementsByTagName(s)[0],
          j = d.createElement(s),
          dl = l != "dataLayer" ? "&l=" + l : "";
        j.async = true;
        j.src = "https://www.googletagmanager.com/gtm.js?id=" + i + dl;
        f.parentNode.insertBefore(j, f);
      })(window, document, "script", "dataLayer", "GTM-NT3H9XXW");
    </script>
    <title>Car Shipping Quote | Cayad Auto Transport</title>
  </head>
  <link rel="canonical" href="https://cayadservices.com/quote2" />

  <body>
    <!-- Google Tag Manager (noscript) -->
    <noscript
      ><iframe
        src="https://www.googletagmanager.com/ns.html?id=GTM-NT3H9XXW"
        height="0"
        width="0"
        style="display:none;visibility:hidden"></iframe></noscript
    >
    <div
      class="h-screen box-border xs:h-max sm:h-max md:h-max w-full flex flex-col justify-center items-center bg-neutral-200"
    >
      <div
        class="max-w-[850px] min-w-[750px] xs:min-w-full xs:min-h-max sm:min-w-full sm:min-h-max md:min-w-full bg-white min-h-[700px] h-max rounded-lg"
      >
        <div
          class="w-[100%] px-8 items-center justify-between xs:pl-4 sm:pl-4 xs:h-max sm:h-max md:h-max xs:mt-10 sm:mt-10 md:mt-24 flex xs:items-start sm:items-start mt-4"
        >
          <div>
            <div class="max-h-[50px] max-w-[200px] xs:mb-4 mt-2">
              <a href="/">
                <img
                  src="/img/logo-cayad.webp"
                  alt="cayad auto transport logo"
                  width="2688"
                  height="609"
                  srcset=""
                />
              </a>
            </div>
            <ShowNumberLead client:load />
          </div>
          <div class="AuthorizeNetSeal xs:w-[70%]" style="float: left;">
            <script type="text/javascript">
              var ANS_customer_id = "40b07bd0-492e-41ef-af3d-203518035d55";
            </script>
            <script
              type="text/javascript"
              src="https://verify.authorize.net:443/anetseal/seal.js"></script>
          </div>
        </div>

        <div
          class="w-full grid grid-cols-2 xs:grid-cols-1 sm:grid-cols-1 md:grid-cols-1 mt-4"
        >
          <div class="w-full xs:pl-0 sm:pl-0 pl-4 flex flex-col items-center">
            <ShowDataQuote client:load />
            <div
              class="w-[95%] xs:w-[95%] sm:w-[85%] md:w-[85%] w-container"
              style="text-align: left; background-color: white; color: black"
            >
              <div class="max-h-9">
                <img
                  src="../img/credit_cards.webp"
                  class="max-h-9"
                  width="1071"
                  height="155"
                  alt="car shipping methods of pay credit cards"
                /><br />
              </div>

              <span class="text-[10px] w-[95%] xs:w-[90%] sm:w-[90%] md:w-[85%]"
                >Fee of 4% will be charged over CC / Paypal /
                <br />Venmo No fee will be charged over Zelle and Cash App</span
              >
            </div>
            <br />
          </div>
          <ShowDestination client:load />
        </div>
        <div class="w-full h-5 flex mb-8 mt-4 justify-center">
          <a href="/" class="text-[#007bff]">Cayad Auto Transport</a>
        </div>
      </div>
    </div>
  </body>
</html>
